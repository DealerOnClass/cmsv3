doctype html
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(name="viewport" content="width=device-width,initial-scale=1.0")
		meta(http-equiv="X-UA-Compatible" content="IE=7")
		link(rel="stylesheet" href="css/main.css")
		title Fv3
	body
		main.container
			section
				header.m-y-2
					h3 Grid
				.row
					.col-xs-8.box .col-xs-8
					.col-xs-4.box .col-xs-4
				header.m-y-2
					h3 Grid Nesting
				.row
					.col-xs-9.box .col-xs-9
						.row
							.col-xs-8.box .col-xs-8
							.col-xs-4.box .col-xs-4
				header.m-y-2
					h3 Grid Reordering
				.row
					.col-xs-4.col-xs-push-4.box .col-xs-4.col-xs-push-4
					.col-xs-8.col-xs-pull-8.box .col-xs-8.col-xs-push-8
			section
				header.m-y-2
					h3 Breadcrumb
				nav.m-b-1.breadcrumb
					a.breadcrumb-item.breadcrumb-link(href="") Home
					a.breadcrumb-item.breadcrumb-link(href="") Library
					span.breadcrumb-item.active Books
			section
				header.m-y-2
					h3 Button
				button.m-b-1.btn Button
				button.m-b-1.btn.btn-block Button
			section
				header.m-y-2
					h3 Button Group
				.btn-group
					button.btn Button
					button.btn Button
					button.btn Button
				.btn-group-stacked
					button.btn Button
					button.btn Button
					button.btn Button
